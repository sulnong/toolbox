# AI服务配置
# OpenAI
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1

# Google Cloud Vision
GOOGLE_CLOUD_VISION_KEY=your_google_vision_key_here

# Azure Computer Vision
AZURE_COMPUTER_VISION_KEY=your_azure_vision_key_here
AZURE_COMPUTER_VISION_ENDPOINT=https://your-region.api.cognitive.microsoft.com/

# 百度OCR
BAIDU_OCR_APP_ID=your_baidu_app_id_here
BAIDU_OCR_API_KEY=your_baidu_api_key_here
BAIDU_OCR_SECRET_KEY=your_baidu_secret_key_here


# 默认AI服务提供商
DEFAULT_AI_PROVIDER=openai

# 文件处理配置
TEMP_DIR=./temp
MAX_FILE_SIZE=100MB
ALLOWED_FILE_TYPES=pdf,jpg,jpeg,png,webp

# 日志配置
LOG_LEVEL=info